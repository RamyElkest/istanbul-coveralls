{
  "name": "istanbul-coveralls",
  "version": "1.0.1",
  "description": "A simple alias for istanbul + coveralls",
  "repository": "shinnn/istanbul-coveralls",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "bin": "./cli.js",
  "scripts": {
    "pretest": "eslint *.js & jscs *.js",
    "test": "_mocha test.js --timeout 7500",
    "coverage": "istanbul cover _mocha test.js -- --timeout 7500",
    "coveralls": "${npm_package_scripts_coverage} && ${npm_package_bin}"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/istanbul-coveralls/blob/master/LICENSE"
    }
  ],
  "files": [
    "cli.js",
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "istanbul",
    "coveralls",
    "command",
    "cli",
    "coverage",
    "lcov",
    "shorten",
    "alias"
  ],
  "dependencies": {
    "chalk": "^0.5.1",
    "coveralls": "^2.11.2",
    "minimist": "^1.1.0",
    "rimraf": "^2.2.8"
  },
  "devDependencies": {
    "eslint": "^0.8.2",
    "fs-extra": "^0.12.0",
    "istanbul": "^0.3.2",
    "jscs": "^1.6.2",
    "mocha": "^1.21.4",
    "require-main": "^0.1.1"
  }
}
