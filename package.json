{
  "name": "istanbul-coveralls",
  "version": "1.0.2",
  "description": "A simple alias for istanbul + coveralls",
  "repository": "shinnn/istanbul-coveralls",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "bin": "cli.js",
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "_mocha test.js --timeout 25600",
    "coverage": "istanbul cover _mocha test.js -- --timeout 25600",
    "coveralls": "${npm_package_scripts_coverage} && ${npm_package_bin}"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/istanbul-coveralls/blob/master/LICENSE"
    }
  ],
  "files": [
    "cli.js",
    "index.js"
  ],
  "keywords": [
    "istanbul",
    "coveralls",
    "command",
    "command-line",
    "cli",
    "coverage",
    "report",
    "integration",
    "lcov",
    "shorten",
    "alias"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "coveralls": "^2.11.2",
    "minimist": "^1.1.0",
    "rimraf": "^2.2.8",
    "sum-up": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^0.15.1",
    "istanbul": "^0.3.6",
    "jscs": "^1.11.3",
    "mocha": "^2.1.0",
    "output-file": "^1.1.1"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98
  }
}
